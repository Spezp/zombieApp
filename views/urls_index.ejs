<!-- views/urls_index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp - Home</title>
</head>
<body>
  <% include ./partials/_header %>
  <h1>List of URLS</h1>
  <ul>
    <% for(var url in urls) { %>
      <li><%= url %> - <%= urls[url] %>
        <form method="POST" action="/urls/<%= url %>/delete">
          <input type="submit" value="Delete">
        </form>

      </li>
    <% }; %>
  </ul>
  <a href="#">Make new TinyApp</a>

</body>
</html>
