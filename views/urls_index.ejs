<!-- views/urls_index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp - Home</title>
</head>
<body style="font-size:20px; padding:50px;">

  <% include partials/_header %>
  <h1>List of URLS</h1>
  <ul>
    <% for(var url in urls) { %>

        <form method="POST" action="/urls/<%= url %>/delete">
          <li><%= url %> - <%= urls[url] %>
          <input type="submit" value="Delete">
        </form>

      </li>
    <% }; %>
  </ul>
  <a href="/urls/new">Make new TinyApp</a>

</body>
</html>
